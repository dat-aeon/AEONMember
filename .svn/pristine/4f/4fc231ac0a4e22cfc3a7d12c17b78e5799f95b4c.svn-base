package mm.com.aeon.vcsaeon.beans;

import java.util.List;

public class MainMenuItemBean {

    private String menuName;

    private List<SubMenuItemBean> subMenuNameList;

    private boolean hasMessageCount;

    private int iconImage;

    private int index;

    private int messageCount;

    public MainMenuItemBean(){}

    public MainMenuItemBean(String menuName, int iconImage, int index, boolean hasMessageCount, List<SubMenuItemBean> subMenuNameList) {
        this.menuName = menuName;
        this.iconImage = iconImage;
        this.index = index;
        this.hasMessageCount = hasMessageCount;
        this.subMenuNameList = subMenuNameList;
    }

    public String getMenuName() {
        return menuName;
    }

    public void setMenuName(String menuName) {
        this.menuName = menuName;
    }

    public List<SubMenuItemBean> getSubMenuNameList() {
        return subMenuNameList;
    }

    public void setSubMenuNameList(List<SubMenuItemBean> subMenuNameList) {
        this.subMenuNameList = subMenuNameList;
    }

    public boolean isHasMessageCount() {
        return hasMessageCount;
    }

    public void setHasMessageCount(boolean hasMessageCount) {
        this.hasMessageCount = hasMessageCount;
    }

    public int getIndex() {
        return index;
    }

    public void setIndex(int index) {
        this.index = index;
    }

    public int getIconImage() {
        return iconImage;
    }

    public void setIconImage(int iconImage) {
        this.iconImage = iconImage;
    }

    public int getMessageCount() {
        return messageCount;
    }

    public void setMessageCount(int messageCount) {
        this.messageCount = messageCount;
    }
}
