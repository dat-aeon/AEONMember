                    <ImageView
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_gravity="right"
                        android:src="@drawable/right_sign_32" />

                </LinearLayout>
            </LinearLayout>-->

            <com.kofigyan.stateprogressbar.StateProgressBar
                android:id="@+id/guar_stepped_bar"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:spb_currentStateNumber="four"
                app:spb_maxStateNumber="five"
                app:spb_stateBackgroundColor="@color/color_disable"
                app:spb_stateForegroundColor="@color/da_button__color"
                app:spb_stateNumberBackgroundColor="@color/white"
                app:spb_stateNumberForegroundColor="@color/white"
                app:spb_currentStateDescriptionColor="@color/da_button__color"
                app:spb_stateDescriptionColor="@color/color_disable"
                app:spb_animateToCurrentProgressState="true"
                app:spb_checkStateCompleted="false"
                app:spb_stateDescriptionSize="12sp"
                app:spb_descriptionTopSpaceIncrementer="2dp"/>

            <TextView
                android:id="@+id/da_gua_data_title"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="@string/da_guarantor_title"
                android:fontFamily="@font/pyidaungsu_bold"
                android:layout_marginBottom="5dp"
                android:textColor="@color/da_label_color"
                android:textSize="@dimen/text_regular_3x"
                android:textStyle="bold" />

        </LinearLayout>

        <ScrollView
            android:id="@+id/guarator_scroll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scrollbars="none"
            android:layout_below="@id/gua_data_banner"
            android:layout_above="@id/button_layout">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/margin_medium"
                android:background="@null"
                android:orientation="vertical"
                android:padding="@dimen/margin_medium">

                <!-- <LinearLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:layout_gravity="center_vertical"
                     android:layout_marginBottom="@dimen/margin_medium_2"
                     android:orientation="vertical">

                     <TextView
                         android:id="@+id/guarantor_title"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"
                         android:gravity="center_horizontal"
                         android:text="Guarantor"
                         android:textColor="@color/colorPrimary"
                         android:textSize="@dimen/text_regular_3x"
                         android:textStyle="bold" />
                 </LinearLayout>-->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_medium_3"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/guarantor_text_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="@string/register_name_label"
                        android:fontFamily="@font/pyidaungsu_bold"
                        android:textColor="@color/da_label_color"
                        android:textSize="@dimen/text_regular"
                        android:textStyle="bold" />

                    <!--Name error field-->
                    <TextView
                        android:id="@+id/guarantor_err_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="Name is required."
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:textColor="@color/da_err_msg_color"
                        android:textSize="@dimen/text_regular"
                        android:visibility="gone" />

                    <EditText
                        android:id="@+id/guarantor_input_name"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/edit_text_height"
                        android:background="@drawable/mandatroy_edit_text_style"
                        android:hint="@string/register_name_holder"
                        android:inputType="textCapCharacters"
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:maxLength="256"
                        android:paddingLeft="@dimen/margin_medium"
                        android:singleLine="true"
                        android:textColor="@color/da_value_color"
                        android:textColorHint="@color/grayLight"
                        android:textSize="@dimen/text_regular" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_medium_3"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/guarantor_text_dob"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/margin_small"
                            android:text="@string/register_dob_label"
                            android:fontFamily="@font/pyidaungsu_bold"
                            android:textColor="@color/da_label_color"
                            android:textSize="@dimen/text_regular"
                            android:textStyle="bold" />

                        <!--Dob Age Limitation-->
                        <TextView
                            android:id="@+id/applicant_dob"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/margin_small"
                            android:layout_marginBottom="@dimen/margin_small"
                            android:fontFamily="@font/pyidaungsu_regular"
                            android:text="@string/register_age_limit"
                            android:textColor="@color/da_err_msg_color"
                            android:textSize="@dimen/text_small" />

                    </LinearLayout>

                    <!--DOB Error Field-->
                    <TextView
                        android:id="@+id/guarantor_err_dob"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="Date of Birth is required."
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:textColor="@color/da_err_msg_color"
                        android:textSize="@dimen/text_regular"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <EditText
                            android:id="@+id/guarantor_input_dob"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/edit_text_height"
                            android:layout_weight="0.9"
                            android:height="40dp"
                            android:background="@drawable/mandatroy_edit_text_style"
                            android:focusable="false"
                            android:hint="@string/register_dob_holder"
                            android:fontFamily="@font/pyidaungsu_regular"
                            android:inputType="text"
                            android:paddingLeft="@dimen/margin_medium"
                            android:textColor="@color/da_value_color"
                            android:textColorHint="@color/grayLight"
                            android:textSize="@dimen/text_regular" />

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_medium_3"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/guarantor_text_nrc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="@string/register_nrc_label"
                        android:fontFamily="@font/pyidaungsu_bold"
                        android:textColor="@color/da_label_color"
                        android:textSize="@dimen/text_regular"
                        android:textStyle="bold" />

                    <!--To check applicant NRC Error-->
                    <TextView
                        android:id="@+id/guarantor_err_nrc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="Nrc is required."
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:textColor="@color/da_err_msg_color"
                        android:textSize="@dimen/text_regular"
                        android:visibility="gone" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/nrc_layout_height"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:weightSum="1">

                        <Spinner
                            android:id="@+id/guarantor_div_code"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/edit_text_height"
                            android:layout_marginRight="@dimen/margin_tiny"
                            android:layout_weight="0.1"
                            android:background="@drawable/edit_text_style"
                            android:gravity="center"
                            android:padding="@dimen/margin_small" />

                        <AutoCompleteTextView
                            android:id="@+id/guarantor_twsp_code"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/edit_text_height"
                            android:layout_marginRight="@dimen/margin_tiny"
                            android:layout_weight="0.7"
                            android:background="@drawable/mandatroy_edit_text_style"
                            android:focusable="true"
                            android:focusableInTouchMode="true"
                            android:gravity="center"
                            android:longClickable="false"
                            android:maxLength="10"
                            android:paddingLeft="@dimen/margin_medium_2"
                            android:paddingRight="@dimen/margin_medium_2"
                            android:singleLine="true"
                            android:textAlignment="center"
                            android:textColor="@color/da_value_color"
                            android:fontFamily="@font/pyidaungsu_regular"
                            android:textSize="@dimen/text_regular" />

                        <Spinner
                            android:id="@+id/guarantor_nrc_type"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/edit_text_height"
                            android:layout_marginRight="@dimen/margin_tiny"
                            android:layout_weight="0.1"
                            android:background="@drawable/edit_text_style"
                            android:gravity="center"
                            android:padding="@dimen/margin_small" />

                        <EditText
                            android:id="@+id/guarantor_nrc_no"
                            android:layout_width="@dimen/nrc_code_width"
                            android:layout_height="@dimen/edit_text_height"
                            android:layout_weight="0.1"
                            android:background="@drawable/mandatroy_edit_text_style"
                            android:gravity="center"
                            android:hint="123456"
                            android:inputType="numberDecimal"
                            android:maxLength="6"
                            android:paddingLeft="@dimen/margin_small"
                            android:paddingRight="@dimen/margin_small"
                            android:textAlignment="center"
                            android:textColor="@color/da_value_color"
                            android:textColorHint="@color/grayLight"
                            android:fontFamily="@font/pyidaungsu_regular"
                            android:textSize="@dimen/text_regular" />

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_medium_3"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/guarantor_nationality"
                            android:layout_width="120dp"
                            android:layout_height="match_parent"
                            android:paddingTop="6dp"
                            android:text="Nationality *"
                            android:fontFamily="@font/pyidaungsu_bold"
                            android:textColor="@color/da_label_color"
                            android:textSize="@dimen/text_regular"
                            android:textStyle="bold" />

                        <RadioGroup
                            android:id="@+id/guar_radioNationality"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/guar_radioMyanmar"
                                android:layout_width="100dp"
                                android:layout_height="wrap_content"
                                android:buttonTint="@color/da_label_color"
                                android:checked="true"
                                android:text="Myanmar"
                                android:fontFamily="@font/pyidaungsu_regular"
                                android:textColor="@color/da_label_color" />

                            <RadioButton
                                android:id="@+id/guar_radioOther"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:buttonTint="@color/da_label_color"
                                android:text="Other"
                                android:fontFamily="@font/pyidaungsu_regular"
                                android:textColor="@color/da_label_color" />
                        </RadioGroup>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/guarantor_err_nationality"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/margin_small"
                            android:text="Description is required."
                            android:fontFamily="@font/pyidaungsu_regular"
                            android:textColor="@color/red"
                            android:textSize="@dimen/text_regular"
                            android:visibility="gone" />

                        <EditText
                            android:id="@+id/guarantor_nationality_detail"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/edit_text_height"
                            android:background="@drawable/mandatroy_edit_text_style"
                            android:hint="Description"
                            android:maxLength="256"
                            android:paddingLeft="@dimen/margin_medium"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:fontFamily="@font/pyidaungsu_regular"
                            android:textColorHint="@color/grayLight"
                            android:textSize="@dimen/text_regular"
                            android:visibility="gone" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_medium_3"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/guarantor_text_mobile_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="Mobile No. *"
                        android:fontFamily="@font/pyidaungsu_bold"
                        android:textColor="@color/da_label_color"
                        android:textSize="@dimen/text_regular"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/guarantor_err_mobile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="Mobile No. is required."
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:textColor="@color/da_err_msg_color"
                        android:textSize="@dimen/text_regular"
                        android:visibility="gone" />

                    <EditText
                        android:id="@+id/guarantor_input_mobile"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/edit_text_height"
                        android:background="@drawable/mandatroy_edit_text_style"
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:hint="@string/register_phoneno_holder"
                        android:inputType="phone"
                        android:maxLength="11"
                        android:paddingLeft="@dimen/margin_medium"
                        android:singleLine="true"
                        android:textColor="@color/da_value_color"
                        android:textColorHint="@color/grayLight"
                        android:textSize="@dimen/text_regular" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_medium_3"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/guar_resident_mobile"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="Resident Tel No."
                        android:fontFamily="@font/pyidaungsu_bold"
                        android:textColor="@color/da_label_color"
                        android:textSize="@dimen/text_regular"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/guarantor_err_resident_tel"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="Mobile No. is required."
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:textColor="@color/da_err_msg_color"
                        android:textSize="@dimen/text_regular"
                        android:visibility="gone" />

                    <EditText
                        android:id="@+id/guarantor_residentTel"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/edit_text_height"
                        android:background="@drawable/edit_text_style"
                        android:hint="@string/register_phoneno_holder"
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:inputType="phone"
                        android:maxLength="11"
                        android:paddingLeft="@dimen/margin_medium"
                        android:singleLine="true"
                        android:textColor="@color/da_value_color"
                        android:textColorHint="@color/grayLight"
                        android:textSize="@dimen/text_regular" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_medium_3"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_tiny"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/text_relation_with"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/margin_small"
                            android:text="Relationship with Applicant *"
                            android:fontFamily="@font/pyidaungsu_bold"
                            android:textColor="@color/da_label_color"
                            android:textSize="@dimen/text_regular"
                            android:textStyle="bold" />

                        <Spinner
                            android:id="@+id/spinner_relation_with"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/edit_text_height"
                            android:layout_weight="0.1"
                            android:background="@drawable/spinner_drop_arrow"
                            android:gravity=""
                            android:paddingLeft="@dimen/margin_small"
                            android:paddingRight="@dimen/margin_small" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/guarantor_err_relation_detail"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/margin_small"
                            android:text="Description is required."
                            android:fontFamily="@font/pyidaungsu_regular"
                            android:textColor="@color/da_err_msg_color"
                            android:textSize="@dimen/text_regular"
                            android:visibility="gone" />

                        <EditText
                            android:id="@+id/guarantor_relation_detail"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/edit_text_height"
                            android:background="@drawable/mandatroy_edit_text_style"
                            android:hint="Description"
                            android:fontFamily="@font/pyidaungsu_regular"
                            android:maxLength="256"
                            android:paddingLeft="@dimen/margin_medium"
                            android:singleLine="true"
                            android:textColor="@color/da_value_color"
                            android:textColorHint="@color/grayLight"
                            android:textSize="@dimen/text_regular"
                            android:visibility="gone" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_medium_3"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/guarantor_address_title"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/edit_text_height"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:gravity="center_vertical"
                        android:fontFamily="@font/pyidaungsu_bold"
                        android:text="Current Address"
                        android:textColor="@color/da_label_color"
                        android:textSize="@dimen/text_medium"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginBottom="@dimen/margin_medium"
                        android:weightSum="1.0">
                        <TextView
                            android:id="@+id/guar_txt_apartment_no"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/edit_text_height"
                            android:layout_weight="0.5"
                            android:gravity="center_vertical"
                            android:fontFamily="@font/pyidaungsu_bold"
                            android:text="@string/da_applicant_apartment_no"
                            android:textColor="@color/da_label_color"
                            android:textSize="@dimen/text_regular"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/guar_apartment_no"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/edit_text_height"
                            android:layout_weight="0.5"
                            android:background="@drawable/edit_text_style"
                            android:hint=""
                            android:fontFamily="@font/pyidaungsu_regular"
                            android:maxLength="20"
                            android:paddingLeft="@dimen/margin_medium"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textColorHint="@color/grayLight"
                            android:textSize="@dimen/text_regular" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginBottom="@dimen/margin_medium"
                        android:weightSum="1.0">

                        <TextView
                            android:id="@+id/guar_txt_room_no"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/edit_text_height"
                            android:layout_weight="0.5"
                            android:gravity="center_vertical"
                            android:fontFamily="@font/pyidaungsu_bold"
                            android:text="@string/da_applicant_room_no"
                            android:textColor="@color/da_label_color"
                            android:textSize="@dimen/text_regular"
                            android:textStyle="bold" />

