<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/stepview_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/edit_text_style"
            android:orientation="vertical"
            android:padding="@dimen/margin_medium"
            android:layout_alignParentTop="true"
            android:visibility="gone">

            <io.canner.stepsview.StepsView xmlns:custom="http://schemas.android.com/apk/res-auto"
                android:id="@+id/stepsView_4"
                android:layout_width="match_parent"
                android:layout_height="64dp"
                custom:circleRadius="20"
                custom:labelSize="10" />

        </LinearLayout>

        <include
            android:id="@+id/service_unavailable_guarantor"
            layout="@layout/service_unavailable"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="gone"
            android:layout_below="@id/stepview_layout"/>

        <LinearLayout
            android:id="@+id/gua_data_banner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:orientation="vertical">

            <!--<LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="10dp">

                <LinearLayout
                    android:id="@+id/back_emer_data"
                    android:layout_width="wrap_content"
