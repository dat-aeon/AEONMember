<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/relativeLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="left|center_vertical"
    android:background="@drawable/grid_layout_border"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:backgroundTint="@color/register_color"
    android:padding="3dp">

    <ImageView
        android:layout_height="50dp"
        android:id="@+id/imageView1"
        android:layout_width="50dp"
        android:src="@drawable/register_512"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="2dp">
    </ImageView>

    <TextView
        android:text="@string/home_register_button"
        android:layout_height="wrap_content"
        android:id="@+id/text_register"
        android:layout_width="wrap_content"
        android:layout_marginTop="12dp"
        android:textSize="6pt"
        android:layout_marginRight="3dp"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/imageView1"
        android:fontFamily="@font/pyidaungsu_regular"
        android:textColor="@color/white"
        android:ellipsize="marquee"/>

</RelativeLayout>