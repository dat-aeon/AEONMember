                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:id="@+id/lbl_finance_term"
                    android:fontFamily="@font/pyidaungsu_bold"
                    android:text="-"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="5"
                android:layout_marginTop="@dimen/margin_medium"
                android:id="@+id/layout_processing_fee"
                android:layout_below="@+id/layout_finance_term">

                <TextView
                    android:id="@+id/processing_fees"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/places_ui_default_primary_dark"
                    android:layout_weight="3"
                    android:fontFamily="@font/pyidaungsu_regular"
                    android:text="Processing Fees"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:id="@+id/lbl_processing_fee_val"
                    android:fontFamily="@font/pyidaungsu_bold"
                    android:text="-"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="5"
                android:layout_marginTop="@dimen/margin_medium"
                android:id="@+id/layout_comp_saving"
                android:layout_below="@+id/layout_processing_fee">

                <TextView
                    android:id="@+id/compulsory_saving"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/places_ui_default_primary_dark"
                    android:layout_weight="3"
                    android:fontFamily="@font/pyidaungsu_regular"
                    android:text="Compulsory Saving"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:id="@+id/lbl_com_saving_val"
                    android:fontFamily="@font/pyidaungsu_bold"
                    android:text="-"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="5"
                android:layout_marginTop="@dimen/margin_medium"
                android:id="@+id/layout_sett_amt"
                android:layout_below="@+id/layout_comp_saving">

                <TextView
                    android:id="@+id/settlement_amount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/places_ui_default_primary_dark"
                    android:layout_weight="3"
                    android:fontFamily="@font/pyidaungsu_regular"
                    android:text="Settlement Amount"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:id="@+id/lbl_sett_amt_val"
                    android:fontFamily="@font/pyidaungsu_bold"
                    android:text="-"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/margin_medium_2"
                android:id="@+id/layout_member_card"
                android:visibility="gone"
                android:layout_below="@+id/layout_sett_amt">

                <TextView
                    android:id="@+id/lbl_img_member_card"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:fontFamily="@font/pyidaungsu_regular"
                    android:text="Member Card"/>

                <mm.com.aeon.vcsaeon.views.customviews.SendMessageImageView
                    android:layout_width="192dp"
                    android:layout_height="128dp"
                    android:scaleType="centerCrop"
                    android:id="@+id/img_member_card"
                    android:src="@drawable/aeon_logo"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/margin_medium_2"
                android:id="@+id/layout_invoice"
                android:visibility="gone"
                android:layout_below="@+id/layout_member_card">

                <TextView
                    android:id="@+id/lbl_img_invoice_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:fontFamily="@font/pyidaungsu_regular"
                    android:text="Invoice"/>

                <mm.com.aeon.vcsaeon.views.customviews.SendMessageImageView
                    android:layout_width="192dp"
                    android:layout_height="128dp"
                    android:id="@+id/img_invoice_view"
                    android:scaleType="centerCrop"
                    android:src="@drawable/aeon_logo"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/margin_medium_2"
                android:visibility="gone"
                android:id="@+id/layout_cash_receipt"
                android:layout_below="@+id/layout_invoice">

                <TextView
                    android:id="@+id/lbl_img_cash_receipt"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:fontFamily="@font/pyidaungsu_regular"
                    android:text="Cash Receipt"/>

                <mm.com.aeon.vcsaeon.views.customviews.SendMessageImageView
                    android:layout_width="192dp"
                    android:layout_height="128dp"
                    android:id="@+id/img_cash_receipt"
                    android:scaleType="centerCrop"
                    android:src="@drawable/aeon_logo"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/margin_medium_2"
                android:id="@+id/layout_agreement"
                android:visibility="gone"
                android:layout_below="@+id/layout_cash_receipt">

                <TextView
                    android:id="@+id/lbl_img_agreement"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:fontFamily="@font/pyidaungsu_regular"
                    android:text="Agreement Letter"/>

                <mm.com.aeon.vcsaeon.views.customviews.SendMessageImageView
                    android:layout_width="192dp"
                    android:layout_height="128dp"
                    android:id="@+id/img_agreement"
                    android:scaleType="centerCrop"
                    android:src="@drawable/aeon_logo"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/margin_medium_2"
                android:id="@+id/layout_uloan"
                android:visibility="gone"
                android:layout_below="@+id/layout_agreement">

                <TextView
                    android:id="@+id/lbl_img_uloan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:fontFamily="@font/pyidaungsu_regular"
                    android:text="Agreement"/>

                <mm.com.aeon.vcsaeon.views.customviews.SendMessageImageView
                    android:layout_width="192dp"
                    android:layout_height="128dp"
                    android:id="@+id/img_uloan"
                    android:scaleType="centerCrop"
                    android:src="@drawable/aeon_logo"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="@dimen/margin_medium_2"
                android:id="@+id/layout_others_info"
                android:visibility="gone"
                android:layout_below="@+id/layout_uloan">

                <TextView
                    android:id="@+id/lbl_others_attach"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:layout_weight="2"
                    android:fontFamily="@font/pyidaungsu_regular"
                    android:text="Others"/>

                <androidx.recyclerview.widget.RecyclerView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/rv_others_attach"/>

            </LinearLayout>

        </RelativeLayout>

    </androidx.core.widget.NestedScrollView>

</RelativeLayout>