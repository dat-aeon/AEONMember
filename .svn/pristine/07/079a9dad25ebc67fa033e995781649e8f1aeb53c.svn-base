                        android:hint="@string/register_phoneno_holder"
                        android:inputType="phone"
                        android:maxLength="11"
                        android:paddingLeft="@dimen/margin_medium"
                        android:textColor="@color/da_value_color"
                        android:textColorHint="@color/grayLight"
                        android:textSize="@dimen/text_regular" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/margin_medium_x"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/emer_other_no"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="Other Phone No."
                        android:textColor="@color/da_label_color"
                        android:fontFamily="@font/pyidaungsu_bold"
                        android:textSize="@dimen/text_regular"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/emergency_err_otherPhoneNo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/margin_small"
                        android:text="Mobile No. is required."
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:textColor="@color/da_err_msg_color"
                        android:textSize="@dimen/text_regular"
                        android:visibility="gone" />

                    <EditText
                        android:id="@+id/emergency_other_no"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/edit_text_height"
                        android:height="@dimen/edit_text_height"
                        android:background="@drawable/edit_text_style"
                        android:fontFamily="@font/pyidaungsu_regular"
                        android:hint="@string/register_phoneno_holder"
                        android:inputType="phone"
                        android:maxLength="11"
                        android:paddingLeft="@dimen/margin_medium"
                        android:textColor="@color/da_value_color"
                        android:textColorHint="@color/grayLight"
                        android:textSize="@dimen/text_regular" />

                </LinearLayout>

            </LinearLayout>

        </ScrollView>

        <LinearLayout
            android:id="@+id/button_layout"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_emerdata_save"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="0.5"
                android:background="@drawable/button_border"
                android:backgroundTint="@color/da_button__color"
                android:text="Save"
                android:fontFamily="@font/pyidaungsu_bold"
                android:textColor="@color/white"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btn_energency_next"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="@dimen/margin_tiny"
                android:layout_marginTop="4dp"
                android:layout_weight="0.5"
                android:background="@drawable/button_border"
                android:backgroundTint="@color/da_button__color"
                android:text="Next"
                android:fontFamily="@font/pyidaungsu_bold"
                android:textColor="@color/white"
                android:textStyle="bold" />

        </LinearLayout>
    </RelativeLayout>
</LinearLayout>