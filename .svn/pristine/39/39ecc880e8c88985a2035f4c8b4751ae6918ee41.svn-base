<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:theme="@style/AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar_main_home"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:theme="@style/ToolbarTheme">

            <include
                android:id="@+id/toolbar_new_main"
                layout="@layout/toolbar_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>

    <include android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/service_unavailable_sq"
        layout="@layout/service_unavailable"
        android:visibility="gone"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/form_layout"
            android:layout_margin="@dimen/margin_small"
            android:padding="@dimen/margin_medium"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/margin_medium_2"
                android:layout_gravity="center_vertical"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/sec_qa_form_title"
                    android:text="@string/secquestconfirm_title"
                    android:textColor="@color/colorPrimary"
                    android:textSize="@dimen/text_medium"
                    android:textStyle="bold"
                    android:fontFamily="@font/pyidaungsu_bold"
                    android:gravity="center_horizontal"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/security_qa_layout_fp"
                android:layout_marginTop="@dimen/margin_medium"
                android:layout_marginBottom="@dimen/margin_medium_2"
                android:orientation="vertical">

            </LinearLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/duplicate_question2"
                android:textColor="@color/red"
                android:textSize="@dimen/text_regular"
                android:visibility="gone"
                android:text="@string/secquest_err_que_same"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="1"
                android:layout_marginTop="@dimen/margin_medium_2"
                android:layout_marginBottom="@dimen/margin_medium"
                android:orientation="horizontal">

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:layout_marginRight="@dimen/margin_small"
                    android:id="@+id/btn_reset_pwd_call"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:drawableStart="@drawable/call_now"
                    android:gravity="center"
                    android:fontFamily="@font/pyidaungsu_bold"
                    android:background="@drawable/button_border"
                    android:text="@string/aboutus_all_now_button"/>

                <Button
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5"
                    android:layout_marginLeft="@dimen/margin_small"
                    android:id="@+id/btn_confirm_sec_qa"
                    android:textColor="@color/white"
                    android:enabled="false"
                    android:textStyle="bold"
                    android:fontFamily="@font/pyidaungsu_bold"
                    android:background="@drawable/button_border"
                    android:text="@string/secquestconfirm_confrim_button"/>

            </LinearLayout>

        </LinearLayout>

    </ScrollView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:background="@null"/>

</LinearLayout>
